.search{
  width: 100%;
  height: 112rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.search_cont{
  width: 702rpx;
  height: 64rpx;
  display: flex;
  align-items: center;
  border: 2rpx solid #2154F4;
  border-radius: 32rpx;
  box-sizing: border-box;
  padding:  0 8rpx 0 30rpx;
  justify-content: space-between;
}
.search_cont_input{
  width: 500rpx;
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #000000;
  border: none;
  outline: none;
}
.search_cont_placeholder{
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #999999;
}

